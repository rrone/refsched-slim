{
    "name": "akrabat/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Upload\\": "vendor/codeguy/upload/src",
            "Tests\\": "tests",
            "App\\Command\\": "src/commands"
        }
    },
    "require-dev": {
        "symfony/debug": "^3.1",
        "phpunit/phpunit": "^6",
        "phpunit/php-invoker": "^2",
        "phpunit/php-code-coverage": "^5",
        "components/bootstrap-default": "^3.3",
        "symfony/browser-kit": "^3.1",
        "symfony/event-dispatcher": "^4",
        "symfony/filesystem": "^4",
        "symfony/process": "^4",
        "there4/slim-test-helpers": "^2.1",
        "symfony/flex": "^1.1"
    },
    "require": {
        "php": "^7.0",
        "slim/slim": "^3.0",
        "slim/twig-view": "~1.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "codeguy/upload": "^1.3",
        "illuminate/database": "~5.1",
        "ircmaxell/password-compat": "^1.0",
        "phpoffice/phpspreadsheet": "^1.4",
        "symfony/console": "^4.1",
        "ext-json": "^1.6",
        "ext-PDO": "^7.2",
        "symfony/dom-crawler": "^4.2",
        "ext-curl": "^7.3",
        "symfony/css-selector": "^4.2",
        "joshfraser/php-name-parser": "dev-master"
    }
}
